{
  "name": "@mudssky/shipit",
  "version": "1.0.11",
  "description": "",
  "bin": {
    "shipit": "dist/index.js"
  },
  "private": false,
  "main": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "examples"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prepare": "husky",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "prebuild": "pnpm typecheck && pnpm biome:check && pnpm test",
    "build": "rspack build && pnpm build:types",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.json",
    "build:watch": "rspack build -w",
    "prepublishOnly": "pnpm build",
    "typecheck": "tsc --noEmit",
    "typecheck:fast": "tsgo --noEmit",
    "biome:fixAll": "biome check --write .",
    "biome:check": "biome check .",
    "biome:ci": "biome ci .",
    "qa": "pnpm typecheck:fast && pnpm biome:fixAll && pnpm test",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@rspack/core": "^1.3.4",
    "@types/imapflow": "^1.0.22",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^24.10.1",
    "@typescript/native-preview": "7.0.0-dev.20251114.1",
    "@vitest/coverage-v8": "^4.0.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^4.0.8",
    "@rspack/cli": "^1.3.4",
    "playwright": "^1.51.1"
  },
  "dependencies": {
    "@mudssky/jsutils": "^1.31.0",
    "ali-oss": "^6.18.1",
    "axios": "^1.7.7",
    "commander": "^14.0.2",
    "cosmiconfig": "^9.0.0",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "execa": "^9.6.0",
    "form-data": "^4.0.0",
    "imapflow": "^1.0.187",
    "inquirer": "^12.6.3",
    "ora": "^9.0.0",
    "zod": "^4.1.12"
  },
  "pnpm": {
    "overrides": {
      "npm-run-path": "5.1.0"
    }
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
